<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<GridPane xmlns:fx="http://javafx.com/fxml" fx:controller="sample.Controller">
    <GridPane GridPane.columnIndex="0" GridPane.rowIndex="0">
        <GridPane>
            <Button GridPane.columnIndex="0" GridPane.rowIndex="0" fx:id="myButton" mnemonicParsing="false" text="Browse..." />
            <Button GridPane.columnIndex="1" GridPane.rowIndex="0" fx:id="solveButton" mnemonicParsing="false" text="Solve" />
            <Button GridPane.columnIndex="2" GridPane.rowIndex="0" fx:id="saveButton" mnemonicParsing="false" text="Save" />
        </GridPane>
        <Label GridPane.columnIndex="0" GridPane.rowIndex="1" fx:id="label" text="data"/>
        <ListView minWidth="300" GridPane.columnIndex="0" GridPane.rowIndex="2" fx:id="elementList"/>
        <ListView minWidth="300" GridPane.columnIndex="1" GridPane.rowIndex="2" fx:id="nodeList" />
    </GridPane>
    <ScrollPane GridPane.columnIndex="0" GridPane.rowIndex="1">
        <GridPane GridPane.columnIndex="0" GridPane.rowIndex="0" fx:id="gridPaneMatrix">

        </GridPane>
    </ScrollPane>
</GridPane>